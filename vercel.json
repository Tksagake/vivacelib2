{
    "version": 2,   
    "builds": [
        {
            "src": "app/api/**/*",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/app/api/$1"
        }
    ],
   "functions": {
    "app/api/**/*": {
      "maxDuration": 60
    }
    }
}